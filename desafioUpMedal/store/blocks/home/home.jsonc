  "stack-layout#promocaomobile": {
    "children": ["image#stackpromocaomobile", "link#stackpromocaomobile"]
  },
  "image#stackpromocaomobile": {
    "props": {
      "blockClass": "imagestackpromocaomobile",
      "width": "95%",
      "src": ""
    }
  },
  "link#stackpromocaomobile": {
    "props": {
      "displayMode": "button",
      "buttonProps": {
        "variant": "primary",
        "size": "large"
      },
      "href": "/leads",
      "label": "Cadastre-se!"
    }
  },

  //Configuração do Custom Query mobile, como que o produto aparece na home mobile!!!
  "search-result-layout.mobile": {
    "children": ["flex-layout.row#searchmobile"]
  },
  "flex-layout.row#searchmobile": {
    "children": ["flex-layout.col#searchmobile"]
  },
  "flex-layout.col#searchmobile": {
    "children": ["search-content#mobile", "search-fetch-more"]
  },
  "search-content#mobile": {
    "blocks": ["gallery#mobile", "not-found"]
  },
  "gallery#mobile": {
    "props": {
      "layouts": [
        {
          "name": "one",
          "component": "OneLineSummary",
          "itemsPerRow": 1
        }
      ],
      "OneLineSummary": "product-summary.shelf#mobile"
    }
  },
  "product-summary.shelf#mobile": {
    "children": [
      "product-summary-name",
      "product-images",
      "product-summary-space",
      "product-price",
      "product-summary-space",
      "add-to-cart-button#homemobile"
    ]
  },
  "add-to-cart-button#homemobile": {
    "props": {
      "onClickBehavior": "go-to-product-page",
